<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/etoilefond"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="159dp"
        android:layout_height="155dp"
        app:srcCompat="@drawable/asteroid1"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="50dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="173dp"
        android:layout_height="162dp"
        app:srcCompat="@drawable/asteroid2"
        tools:layout_editor_absoluteX="203dp"
        tools:layout_editor_absoluteY="140dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="190dp"
        android:layout_height="190dp"
        app:srcCompat="@drawable/asteroid3"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="258dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="167dp"
        android:layout_height="170dp"
        app:srcCompat="@drawable/asteroid4"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="344dp" />

    <ImageView
        android:id="@+id/imageViewJoyExt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/pad_exterior" />

    <ImageView
        android:id="@+id/imageViewJoyCen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/pad_center"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="586dp" />

    <ImageView
        android:id="@+id/imageViewTie"
        android:layout_width="228dp"
        android:layout_height="230dp"
        app:srcCompat="@drawable/tie"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="218dp" />
</androidx.constraintlayout.widget.ConstraintLayout>

